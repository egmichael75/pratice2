#%RAML 1.0 SecurityScheme
type: Basic Authentication
description: needs username and password
describedBy:
  headers:
    authorization:
      description: user needs a valid username and pw
      type: string
  responses:
    401:
      description: unautorized user
    403:
      description: forbidden access
  queryParameters:
    username
      type: string
      required: true
       